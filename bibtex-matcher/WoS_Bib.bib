Keywords = {Supply chain; carbon footprint; resilience; process integration; DEMATEL},
Keywords-Plus = {EMISSION REDUCTION; RISK-MANAGEMENT; MODERATING ROLE; MANAGING RISK;
   SUSTAINABILITY; COLLABORATION; VULNERABILITY; MITIGATION; STRATEGIES;
