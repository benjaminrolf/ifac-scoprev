Keywords = {Firm performance; Learning; Agility; Natural disasters; Dynamic
   capabilities; Catastrophic vulnerability},
Keywords-Plus = {RESILIENCE DEVELOPMENT; FLEXIBILITY; DISRUPTION; RISK; UNCERTAINTY;
