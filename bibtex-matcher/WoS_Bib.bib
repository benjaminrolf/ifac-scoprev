Keywords = {industrial ecology; inventory-driven price formation; life cycle
   assessment; material efficiency; material system modeling; recycling
   economics},
Keywords-Plus = {MATERIAL EFFICIENCY; FUTURES PRICES; COPPER PRICE; RESOURCES; ENERGY;
   INDUSTRY; POLICY; WASTE; INDICATORS; FRAMEWORK},
Research-Areas = {Science \& Technology - Other Topics; Engineering; Environmental
   Sciences \& Ecology},
