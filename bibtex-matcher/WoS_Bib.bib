@article{ WOS:000401467500001,
Author = {Zu, Yafei and Chen, Lianghua},
Title = {Myopic versus Far-Sighted Behaviors in Dynamic Supply Chain Coordination
   through Advertising with Reference Price Effect},
Journal = {DISCRETE DYNAMICS IN NATURE AND SOCIETY},
Year = {2017},
Volume = {2017},
Abstract = {To better understand the different effects of themyopic and far-sighted
   behaviors on the advertising coordination in dynamic supply chain, this
   paper takes the reference price effect into consideration and formulates
   four differential game models for the two-level supply chain composed of
   a manufacturer and a retailer in the situation of Stackelberg game. In
   our models, the market demand is assumed to be affected by the goodwill,
   reference price, and the advertising investment, in which the
   advertising investment can promote the construction of goodwill and such
   goodwill can further enhance the reference price. The results show that
   the participating members in the supply chain should invest more in
   advertisement to improve the goodwill and the relative reference price
   reflected in the minds of consumers. A far-sighted manufacturer will
   invest more in the advertisement and charge a higher wholesale price
   regardless of the behavior choice of the retailer. However, such kind of
   ignorance leads to different results on the retail pricing strategies of
   the retailer. The numerical analyses are given in the end to verify the
   effectiveness of the conclusions which provide the theoretical support
   to the dynamic supply chain coordination in practice.},
Type = {Article},
Affiliation = {Zu, YF (Corresponding Author), Southeast Univ, Sch Econ \& Management, Nanjing 211189, Jiangsu, Peoples R China.
   Zu, Yafei; Chen, Lianghua, Southeast Univ, Sch Econ \& Management, Nanjing 211189, Jiangsu, Peoples R China.},
