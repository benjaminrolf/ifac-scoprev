@article{ WOS:000580614000001,
Author = {Finkbeiner, Marco and Pannok, Maik and Fasel, Henrik and Riese, Julia
   and Lier, Stefan},
Title = {Modular Production with Bio-Based Resources in a Decentral Production
   Network},
Journal = {CHEMIE INGENIEUR TECHNIK},
Year = {2020},
Volume = {92},
Number = {12, SI},
Pages = {2041-2045},
Month = {DEC},
Abstract = {The change in process industry from fossil resources to alternative
   feedstock is indispensable due to the scarcity of resources and global
   warming. This leads to new challenges for the production systems. On the
   market side, rapid innovation is accompanied by shorter product life
   cycles leading to an increasing uncertainty of demand in terms of
   product type, volume and location. Therefore, the following five
   elements are combined into a concept to address these challenges:
   transformable production systems, local bio-based resources, CO(2)as
   feedstock, renewable energy and decentral production network with local
   economies.},
