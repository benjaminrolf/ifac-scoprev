EarlyAccessDate = {FEB 2022},
Keywords = {Supply chain agility; Supply chain alignment; Supply chain adaptability;
   Triple-A},
Keywords-Plus = {DISCRIMINANT VALIDITY; RISK-MANAGEMENT; AGILITY; ADAPTABILITY;
   PERFORMANCE; FLEXIBILITY; ALIGNMENT; FIRM; RESILIENCE; CAPABILITIES},
