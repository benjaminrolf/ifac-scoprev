@article{ WOS:000322055500010,
Author = {Klosterhalfen, Steffen T. and Dittmar, Daniel and Minner, Stefan},
Title = {An integrated guaranteed- and stochastic-service approach to inventory
   optimization in supply chains},
Journal = {EUROPEAN JOURNAL OF OPERATIONAL RESEARCH},
Year = {2013},
Volume = {231},
Number = {1},
Pages = {109-119},
Month = {NOV 16},
Abstract = {Multi-echelon inventory optimization literature distinguishes
   stochastic- (SS) and guaranteed-service (GS) approaches as mutually
   exclusive frameworks. While the GS approach considers flexibility
   measures at the stages to deal with stockouts, the SS approach only
   relies on safety stock. Within a supply chain, flexibility levels might
   differ between stages rendering them appropriate candidates for one
   approach or the other. The existing approaches, however, require the
   selection of a single framework for the entire supply chain instead of a
   stage-wise choice. We develop an integrated hybrid-service (HS) approach
   which endogenously determines the overall cost-optimal approach for each
   stage and computes the required inventory levels. We present a dynamic
   programming optimization algorithm for serial supply chains that
   partitions the entire system into subchains of different types. From a
   numerical study we find that, besides implicitly choosing the better of
   the two pure frameworks, whose cost differences can be considerable, the
   HS approach enables additional pipeline and on-hand stock cost savings.
   We further identify drivers for the preferability of the HS approach.
   (C) 2013 Elsevier B.V. All rights reserved.},
Type = {Article},
Affiliation = {Klosterhalfen, ST (Corresponding Author), Univ Mannheim, Sch Business, D-68131 Mannheim, Germany.
   Klosterhalfen, Steffen T., Univ Mannheim, Sch Business, D-68131 Mannheim, Germany.
   Dittmar, Daniel, Univ Wuppertal, Schumpeter Sch Business \& Econ, D-42097 Wuppertal, Germany.
   Minner, Stefan, Tech Univ Munich, TUM Sch Management, D-80333 Munich, Germany.},
DOI = {10.1016/j.ejor.2013.05.032},
Keywords = {Inventory; Multi-echelon; Guaranteed service; Stochastic service;
   Partitioning},
Keywords-Plus = {MANAGEMENT; PLACEMENT},
Research-Areas = {Business \& Economics; Operations Research \& Management Science},
