Keywords = {Supply networks; Network change; Supply chain disruption; Catastrophic
   disaster; Complexity},
Keywords-Plus = {COMPLEX ADAPTIVE SYSTEMS; STRUCTURAL HOLES; RESILIENCE DEVELOPMENT;
