@article{ WOS:000362726400003,
Author = {Schoenherr, Tobias and Swink, Morgan},
Title = {The Roles of Supply Chain Intelligence and Adaptability in New Product
   Launch Success},
Journal = {DECISION SCIENCES},
Year = {2015},
Volume = {46},
Number = {5},
Pages = {901-936},
Month = {OCT},
Abstract = {Many new product introductions continue to be unsuccessful, and while
   researchers have studied product development processes, relatively few
   studies directly address new product launch. We do so in the present
   research and posit that supply chain intelligence, defined as
   technological and competitive knowledge sourced and integrated from
   suppliers, customers, and competitors, plays an important role in
   explaining new product launch success. We further employ the
   knowledge-based view to theorize that both supply chain adaptability and
   product innovation capability act as important mediators of the effects
   of supply chain intelligence on new product launch success and firm
   financial performance. While the former capability refers to a firm's
   ability to quickly adjust its supply chain to react to market and
   product design changes, the latter refers to the firm's proficiency in
   developing innovative new products. We test hypothesized relationships
   among these factors utilizing data collected in a survey of 229 U.S.
   manufacturing firms. Results point to the central role of supply chain
   adaptability in capturing the benefits of supplier technological
   intelligence for enhanced product innovation capability, new product
   launch success, and firm financial performance. In contrast, product
   innovation capability serves as the generative means by which customer
   and competitor intelligence is translated into more successful new
   product launches, which, in turn, produce superior firm financial
   performance. Overall, these findings contribute to a better
   understanding of factors that can explain why certain product launches
   are more successful than others, and offer practical insights for
   appropriate investments in the development of related knowledge
   resources.},
Type = {Article},
Affiliation = {Schoenherr, T (Corresponding Author), Michigan State Univ, Dept Supply Chain Management, Broad Coll Business, 632 Bogue St,Room N370, E Lansing, MI 48824 USA.
   Schoenherr, Tobias, Michigan State Univ, Dept Supply Chain Management, Broad Coll Business, E Lansing, MI 48824 USA.
   Swink, Morgan, Texas Christian Univ, Supply \& Value Chain Ctr, Neeley Sch Business, Ft Worth, TX 76129 USA.},
DOI = {10.1111/deci.12163},
Keywords = {Knowledge-Based View; New Product Launch Success; Product Innovation
   Capability; Supply Chain Adaptability; Supply Chain Intelligence},
Keywords-Plus = {DYNAMIC CAPABILITIES; SERVICE DEVELOPMENT; MODERATING ROLE; BLACK-BOX;
   KNOWLEDGE; INNOVATION; FIRM; INTEGRATION; ANTECEDENTS; INFORMATION},
