DOI = {10.1016/j.cie.2019.06.014},
Keywords = {Dual-channel supply chain; Flexible return policy; Energy-saving
   regulation; Government intervention; Pricing policies; Multi-level game
   theory},
Keywords-Plus = {PRODUCT QUALITY; 2 GREEN; COMPETITION; GOVERNMENT; MODEL; INTERVENTION;
