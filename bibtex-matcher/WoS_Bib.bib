@article{ WOS:000745768900001,
Author = {Awan, Muhammad Asees and Ali, Yousaf},
Title = {Risk Assessment in Supply Chain Networks of China-Pakistan Economic
   Corridor (CPEC)},
Journal = {CHINESE POLITICAL SCIENCE REVIEW},
Abstract = {Complexities in today's Supply Chain Networks (SCN) have exposed them to
   various risks, vulnerabilities, and disruptions. Recently, the Chinese
   \$62 billion investment in the wake of the China-Pakistan Economic
   Corridor (CPEC), which includes a network of highways and railways
   projects, will be a milestone for regional connectivity. Through CPEC,
   China aims to connect its north-western province Xinjiang to global
   supply chain networks via Gwadar port Pakistan. Since 9/11, Pakistan has
   become a terrorism stuck state, resulting in the loss of political and
   economic stability, reducing the infrastructure development and Foreign
   Direct Investment (FDI). Under these circumstances, risk assessment of
   supply chain networks in a responsive way toward these problems is
   crucial for authorities. This paper aims to identify and investigate the
   relationship between supply chain risks using the Interpretive
   Structural Modelling (ISM) technique. Additionally, interdependencies
   among risks have been calculated, and classification is done via
   cross-impact matrix multiplication applied to classification (MICMAC)
   analysis based on corresponding dependency and driving power. Results
   show that political and economic stability has the highest driving power
   and lowest dependence power, indicating that all the risks are directly
   or indirectly associated with it. Simultaneously, epidemics and
   ecosystem changes have the highest dependence power and most
   insufficient driving power, which shows its significant dependency on
   other risks. Finally, based on our model, authorities can design a
   resilient supply chain network responsive to several risks, and
   efficient operations are assured.},
Type = {Article; Early Access},
Affiliation = {Ali, Y (Corresponding Author), Ghulam Ishaq Khan Inst Engn Sci \& Technol GIKI, GIK Inst, Sch Management Sci, Topi 23640, Khyber Pakhtoon, Pakistan.
   Awan, Muhammad Asees; Ali, Yousaf, Ghulam Ishaq Khan Inst Engn Sci \& Technol GIKI, GIK Inst, Sch Management Sci, Topi 23640, Khyber Pakhtoon, Pakistan.},
DOI = {10.1007/s41111-021-00199-w},
EarlyAccessDate = {JAN 2022},
Keywords = {Risk assessment; SCN; CPEC; ISM; MICMAC},
Keywords-Plus = {DESIGN; MANAGEMENT; MODEL; RESILIENCE; GREEN; PERFORMANCE; TECHNOLOGY;
   BARRIERS; ENERGY},
Research-Areas = {Government \& Law},
Web-of-Science-Categories  = {Political Science},
Author-Email = {gif1729@giki.edu.pk
   yousafkhan@giki.edu.pk},
Affiliations = {GIK Institute Engineering Science \& Technology},
Times-Cited = {1},
Unique-ID = {WOS:000745768900001},
}

