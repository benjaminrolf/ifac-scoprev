DOI = {10.1016/j.ijpe.2019.04.027},
Keywords = {Pricing; Advertising; Marketing-operations interface; Game theory},
Keywords-Plus = {CHANNEL PERFORMANCE; COORDINATION; RISK; MANUFACTURERS; SUPPORT;
   INCENTIVES; PROMOTIONS; INVENTORY; RETAILER; BENEFITS},
