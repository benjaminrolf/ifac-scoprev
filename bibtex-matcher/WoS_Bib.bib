DOI = {10.1016/j.ijpe.2020.107689},
Article-Number = {107689},
Keywords = {Additive manufacturing adoption; Supply chain flexibility; Supply chain
   performance; Automotive industry},
Keywords-Plus = {MASS CUSTOMIZATION; MANAGEMENT-PRACTICES; CONCEPTUAL-MODEL; PRODUCT
   VARIETY; IMPACT; INTEGRATION; FRAMEWORK; AGILITY; FIRM; CAPABILITY},
Research-Areas = {Engineering; Operations Research \& Management Science},
Web-of-Science-Categories  = {Engineering, Industrial; Engineering, Manufacturing; Operations Research
