   Baboli, A.; Sazvar, Z., INSA Lyon, DISP Lab, F-69621 Villeurbanne, France.
   Sazvar, Z., Sharif Univ Technol, Dept Ind Engn, Tehran, Iran.},
DOI = {10.1016/j.ejor.2013.03.033},
Keywords = {Supply chain management; Aggregate production planning; Green
   principles; Quantity discount; Nonlinear shortage cost; Demand
   uncertainty},
Keywords-Plus = {GOAL PROGRAMMING APPROACH; GENETIC ALGORITHM; MANAGEMENT; LOGISTICS},
