   Airiau, Christian, GlaxoSmithKline, Prod Dev \& Supply Res \& Dev, Upper Merion, PA USA.},
DOI = {10.1007/s12247-018-9348-7},
Keywords = {Multivariate statistical process monitoring; MSPM; Continuous
   manufacturing; PAT; Process monitoring; Multivariate analysis},
Keywords-Plus = {BATCH; PERSPECTIVES; TRANSITION; PAT},
