Keywords = {Service mass customization (SMC); Supply chain integration (SCI);
   Resource integration value; Strategic renewal; Resource scheduling},
Keywords-Plus = {ORGANIZATIONAL INERTIA; STRUCTURAL INERTIA; FAMILY DESIGN; CAPABILITY;
   MODEL; INFORMATION; PERSPECTIVE; TECHNOLOGY; MANAGEMENT; ALLIANCES},
Research-Areas = {Computer Science; Engineering},
Web-of-Science-Categories  = {Computer Science, Interdisciplinary Applications; Engineering,
   Industrial},
