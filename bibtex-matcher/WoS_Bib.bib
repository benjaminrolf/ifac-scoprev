EarlyAccessDate = {MAY 2020},
Article-Number = {0972150920923075},
Keywords = {Supply chain flexibility; interpretive structure modeling; Indian
   personal hygiene industry; fuzzy MICMAC},
Keywords-Plus = {CONCEPTUAL-MODEL; UNCERTAINTY; MANAGEMENT; PERFORMANCE; ORIENTATION;
   DIMENSIONS; CAPABILITY; SELECTION; STRATEGY; AGILITY},
Research-Areas = {Business \& Economics},
