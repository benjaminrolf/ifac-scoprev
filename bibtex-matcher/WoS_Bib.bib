@article{ WOS:000332282000001,
Author = {Yang, Hang and Fong, Simon and Zhuang, Yan},
Title = {Mediating Dynamic Supply Chain Formation by Collaborative Single Machine
   Earliness/Tardiness Agents in Supply Mesh},
Journal = {MATHEMATICAL PROBLEMS IN ENGINEERING},
Year = {2014},
Volume = {2014},
Abstract = {Nowadays, a trend of forming dynamic supply chains with different
   trading partners over different e-marketplaces has emerged. These supply
   chains, which are called ``supply mesh,{''} generally refer to
   heterogeneous electronic marketplaces in which dynamic supply chains, as
   per project (often make-to-order), are formed across different parties.
   Conceptually, in a supply mesh a dynamic supply chain is formed
   vertically, mediating several companies for a project. Companies that
   are on the same level horizontally are either competitors or cohorts. A
   complex scenario such as this makes it challenging to find the right
   group of members for a dynamic supply chain. Earlier on, a multiagent
   model called the collaborative single machine earliness/tardiness (CSET)
   model was proposed for the optimal formation of make-to-order supply
   chains. This paper contributes the particular agent designs, for
   enabling themediation of CSET in a supplymesh, and the possibilities are
   discussed. It is demonstrated via a computer simulation, based on
   samples fromthe U. S. textile industry, that by using intelligent agents
   under the CSET model it is possible to automatically find an ideal group
   of trading partners from a supply mesh.},
Type = {Article},
Affiliation = {Fong, S (Corresponding Author), Univ Macau, Dept Comp \& Informat Sci, Ave Padre Tomas Pereira, Taipa, Macau, Peoples R China.
   Yang, Hang; Fong, Simon; Zhuang, Yan, Univ Macau, Dept Comp \& Informat Sci, Taipa, Macau, Peoples R China.},
