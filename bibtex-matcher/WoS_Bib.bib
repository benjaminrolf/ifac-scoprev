Keywords-Plus = {MASS CUSTOMIZATION; INFORMATION-TECHNOLOGY; ORGANIZATIONAL DESIGN;
   PRODUCT DEVELOPMENT; MANAGEMENT; TIME; CAPABILITIES; PERFORMANCE;
   INTEGRATION; COORDINATION},
Research-Areas = {Engineering; Operations Research \& Management Science},
Web-of-Science-Categories  = {Engineering, Industrial; Engineering, Manufacturing; Operations Research
   \& Management Science},
Author-Email = {roh@rowan.edu
   Paul.hong@utoledo.edu
   hmin@bgsu.edu},
Affiliations = {Rowan University; University of Toledo; Bowling Green State University},
