@article{ WOS:000620044900016,
Author = {Das, Kanchan},
Title = {Integration of flexibility in reverse supply chain planning process},
Journal = {INTERNATIONAL JOURNAL OF ENVIRONMENTAL TECHNOLOGY AND MANAGEMENT},
Year = {2020},
Volume = {23},
Number = {2-4, SI},
Pages = {245-270},
Abstract = {This research formulates reverse supply chain (RSC) flexibility criteria
   and integrates them in a closed loop supply chain planning model for
   improving overall business performances and sustainability. Proposed RSC
   flexibility criteria include: a) channel selection strategy to collect
   customer returned products (returnable); b) product design and recovery
   strategy to recover components, modules, and products from returnable;
   c) product categorisation strategy for marketing new, recovered, and
   mixed quality level product. Each flexibility criteria is formulated
   independently to provide options to supply chains (SCs) to incorporate
   all or select ones in their planning process. The benefits,
   applicability and integration procedures of proposed flexibility
   considerations are demonstrated through a numerical example.},
Type = {Article},
Affiliation = {Das, K (Corresponding Author), East Carolina Univ, Dept Technol Syst, 245 Slay Hall, Greenville, NC 27858 USA.
