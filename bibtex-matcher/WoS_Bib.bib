Keywords = {Ambidextrous strategy; Supply chain flexibility; Information system
   flexibility; ISO 9001 standard},
Keywords-Plus = {TOTAL QUALITY MANAGEMENT; RESOURCE ORCHESTRATION; COMPETITIVE ADVANTAGE;
   DYNAMIC CAPABILITIES; MANUFACTURING FLEXIBILITY; MODERATING ROLE;
   INNOVATION; AMBIDEXTERITY; PERFORMANCE; ISO-9000},
