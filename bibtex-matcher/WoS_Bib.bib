Keywords = {disaster management; supply chain networks; complex adaptive systems;
   resilience},
Keywords-Plus = {HUMANITARIAN LOGISTICS; COORDINATION; MODEL; CRITICALITY; DYNAMICS;
   SYSTEMS; TRUST; AID},
