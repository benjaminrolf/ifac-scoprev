@article{ WOS:000337184500023,
Author = {Selvarajah, Esaignani and Zhang, Rui},
Title = {Supply chain scheduling to minimize holding costs with outsourcing},
Journal = {ANNALS OF OPERATIONS RESEARCH},
Year = {2014},
Volume = {217},
Number = {1},
Pages = {479-490},
Month = {JUN},
Abstract = {This paper addresses a scheduling problem in a flexible supply chain, in
   which the jobs can be either processed in house, or outsourced to a
   third-party supplier. The goal is to minimize the sum of holding and
   delivery costs. This problem is proved to be strongly NP-hard. Consider
   two special cases, in which the jobs have identical processing times.
   For the problem with limited outsourcing budgets, a NP-hardness proof, a
   pseudo-polynomial algorithm and a fully polynomial time approximation
   scheme are presented. For the problem with unlimited outsourcing
   budgets, the problem is shown to be equivalent to the shortest path
   problem, and therefore it is in class P. This shortest-path-problem
   solution approach is further shown to be applicable to a similar but
   more applicable problem, in which the number of deliveries is upper
   bounded.},
Type = {Article},
Affiliation = {Zhang, R (Corresponding Author), Univ Windsor, Odette Sch Business, 401 Sunset Ave, Windsor, ON N9B 3P4, Canada.
   Selvarajah, Esaignani; Zhang, Rui, Univ Windsor, Odette Sch Business, Windsor, ON N9B 3P4, Canada.},
DOI = {10.1007/s10479-013-1522-1},
Keywords = {Supply chain scheduling; Outsourcing; Inventory control; FPTAS;
   Approximation algorithm; Shortest path problem},
Keywords-Plus = {DELIVERY},
Research-Areas = {Operations Research \& Management Science},
