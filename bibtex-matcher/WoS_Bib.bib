Affiliation = {Montastruc, L (Corresponding Author), Univ Toulouse, Lab Genie Chim, CNRS, INP,UPS, Toulouse, France.
   Di Pretoro, Alessandro; Negny, Stephane; Montastruc, Ludovic, Univ Toulouse, Lab Genie Chim, CNRS, INP,UPS, Toulouse, France.},
DOI = {10.1016/j.compchemeng.2022.107730},
Article-Number = {107730},
Keywords = {Uncertainty modelling; Supply chain; Flexibility; Traveling deliveryman
   problem; Operational research},
