   Weaver, Robert D., Penn State Univ, Dept Agr Econ, University Pk, PA 16802 USA.
   Friesz, Terry L., Penn State Univ, Dept Ind \& Mfg Engn, University Pk, PA 16802 USA.},
DOI = {10.1016/j.ejor.2013.05.036},
Keywords = {Sustainable supply chain; Dynamic game; Network oligopolies;
   Differential variational inequalities; Nonlinear complementary problem},
Keywords-Plus = {ENVIRONMENTAL-REGULATION; REVERSE LOGISTICS; PRODUCT RECOVERY; POLICY;
