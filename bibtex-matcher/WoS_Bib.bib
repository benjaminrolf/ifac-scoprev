@article{ WOS:000373740300011,
Author = {Moreira, Madalena and Tjahjono, Benny},
Title = {Applying performance measures to support decision-making in supply chain
   operations: a case of beverage industry},
Journal = {INTERNATIONAL JOURNAL OF PRODUCTION RESEARCH},
Year = {2016},
Volume = {54},
Number = {8},
Pages = {2345-2365},
Month = {APR 17},
Abstract = {Performance measurement systems (PMS) have commonly been applied to
   evaluate and reward performances at managerial levels, especially in the
   context of supply chain management. However, evidence suggests that the
   effective use of PMS can also positively influence the behaviour and
   improve performance at an operational level. The motivation is to
   accomplish organisational goals, namely to increase supply chain
   flexibility by responding to evermore-varying customer demands in a
   timely manner. The purpose of the study described in this paper was to
   develop a conceptual framework that adopts performance measures for
   ex-ante decision-making at an operational level within the supply chain.
   To guide the research, five questions were asked and subsequently key
   gaps have been identified. In an attempt to fill the gaps, a case study
   at a major global brand beverage company has been carried out, and as a
   result, a conceptual framework of the PMS has been developed. Overall,
   the research offers a foundation of the applicability and impact of PMS
   in the supply chain and provides a framework that attends to some of the
   potential uses of PMS that so far have not been practically applied. The
   outcomes from the testing indicate that the initial gaps identified in
   the literature have been addressed and that the framework is judicious
   with scope for practical applicability. The framework is deemed worthy
   of further testing in different operational contexts of the supply
   chain.},
Type = {Article},
Affiliation = {Tjahjono, B (Corresponding Author), Cranfield Univ, Cranfield Sch Management, Supply Chain Res Ctr, Cranfield MK43 0AL, Beds, England.
   Moreira, Madalena; Tjahjono, Benny, Cranfield Univ, Cranfield Sch Management, Supply Chain Res Ctr, Cranfield MK43 0AL, Beds, England.},
DOI = {10.1080/00207543.2015.1076944},
Keywords = {performance measures; performance measurement systems; decision-making;
   supply chain operations; flexibility},
Keywords-Plus = {MEASUREMENT SYSTEMS; BALANCED SCORECARD; IMPLEMENTATION; CAPABILITIES;
   INTEGRATION; MANAGEMENT; METRICS; FIRMS; MODEL},
Research-Areas = {Engineering; Operations Research \& Management Science},
